{
	"name": "@cdellacqua/time",
	"version": "2.0.3",
	"description": "A Time class for JavaScript",
	"types": "build/index.d.ts",
	"main": "build/index.cjs",
	"module": "build/index.js",
	"type": "module",
	"scripts": {
		"test": "jest --no-cache",
		"coverage": "jest --coverage",
		"build": "rimraf build && rollup -c && tsc --declaration --emitDeclarationOnly --declarationDir build",
		"doc": "rimraf docs && typedoc --disableSources --readme none --mode file --readme none --plugin typedoc-plugin-markdown src/index.ts",
		"prepare": "npm run build && npm run doc",
		"prepublishOnly": "npm test"
	},
	"keywords": [
		"time",
		"timeonly",
		"json"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cdellacqua/time.git"
	},
	"bugs": {
		"url": "https://github.com/cdellacqua/time/issues"
	},
	"homepage": "https://github.com/cdellacqua/time",
	"files": [
		"build/**/*"
	],
	"exports": {
		".": {
			"import": "./build/index.js",
			"require": "./build/index.cjs"
		},
		"./package.json": "./package.json"
	},
	"author": "Carlo Dell'Acqua",
	"license": "MIT",
	"devDependencies": {
		"@rollup/plugin-commonjs": "^16.0.0",
		"@rollup/plugin-node-resolve": "^10.0.0",
		"@rollup/plugin-typescript": "^6.1.0",
		"@types/jest": "^25.2.1",
		"@types/node": "^14.0.23",
		"jest": "^26.0.1",
		"rimraf": "^3.0.2",
		"rollup": "^2.32.1",
		"ts-jest": "^25.5.0",
		"typedoc": "^0.17.8",
		"typedoc-plugin-markdown": "^2.2.17",
		"typescript": "^4.0.3"
	}
}
